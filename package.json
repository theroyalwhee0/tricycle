{
  "name": "@tricycle/tricycle",
  "version": "0.0.13",
  "description": "TricycleJS Azure Functions Framework",
  "author": "Adam Mill <hismajesty@theroyalwhee.com>",
  "license": "Apache-2.0",
  "keywords": [
    "azure-functions"
  ],
  "main": "dist/index.js",
  "peerDependencies": {
    "@azure/functions": ">= 3.2.0 < 4.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/node": "^18.7.6",
    "@types/sinon": "10.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "chai": "4.3.6",
    "eslint": "8.19.0",
    "mocha": "10.0.0",
    "sinon": "14.0.0",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@theroyalwhee0/istype": "3.0.4"
  },
  "scripts": {
    "build": "tsc",
    "rebuild": "npm run clean && tsc --build --force",
    "clean": "mkdir -p ./dist && [ ! -z \"$(find ./dist/ -maxdepth 1 -type f -print -quit)\" ] && rm -R ./dist/*; true",
    "watch": "npm run build && tsc --watch",
    "lint": "eslint .",
    "test": "mocha --require ts-node/register test/**/*.test.ts",
    "test-watch": "mocha --reporter dot --watch --watch-files test/**/*.test.ts --watch-files src/**/*.ts --require ts-node/register test/**/*.test.ts",
    "dryrun": "npm run rebuild && npm prune && npm audit && npm run lint && npm test && npm publish --dry-run",
    "audit": "npm audit --omit dev --omit optional"
  }
}
